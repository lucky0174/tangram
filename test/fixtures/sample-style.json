{
    "earth": {
        "filter": "property('layer', 'earth')",
        "order": 1,
        "style": {
            "color": [255, 255, 255]
        }

    },

    "roads": {
        "filter": "property('layer', 'roads')",
        "order": 5,
        "style": {
            "type": "polygon",
            "color": [1, 1, 1],
            "width": 5
        },
        "highway": {
            "filter": "property('kind', 'highway')",
            "order": 1,
            "fill": {
                "style": {
                    "color": [1, 1, 1],
                    "width": 10
                },
                "offramp": {
                    "filter": "property('offramp', true)",
                    "style": {
                        "color": [1, 0.5, 0]
                    }
                }
            },
            "outline": {
                "order": -1,
                "style": {
                    "type": "line",
                    "color": [1, 1, 0],
                    "width": 12
                }
            }
        },
        "bikepath": {
            "filter": "property('kind', 'cyclepath')",
            "style": {
                "order": 2,
                "color": [0, 1, 1]
            },
            "another-style": {
                "style": {
                    "color": [0.5, 1, 1]
                }
            }
        },
        "stuff": {
            "style": {
                "color": [1, 0, 1]
            }
        }
    },

    "roads2": {
        "filter": "property('layer', 'roads')",
        "order": 1,
        "style": {
            "color": [1, 0, 1]
        },
        "more-stuff": {
            "order": 3.14,
            "style": {
                "color": [1, 0.5, 1]
            }
        }
    }

}


